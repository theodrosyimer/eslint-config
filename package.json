{
  "name": "@thyi/eslint-config",
  "version": "2.0.13",
  "description": "ESLint Config for JavaScript, Typescript, React, React Native",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "prettier",
    "react",
    "react-native",
    "lint",
    "config"
  ],
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./react": "./react.js"
  },
  "author": "Theodros Yimer",
  "license": "MIT",
  "eslintConfig": {
