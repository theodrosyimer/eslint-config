{
  "name": "@thyi/eslint-config",
  "version": "1.26.2",
  "description": "ESLint and Prettier Config for JavaScript and Typescript",
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "prettier",
    "lint",
    "config"
  ],
  "main": "index.js",
  "author": "Theodros Yimer",
  "license": "MIT",
  "eslintConfig": {
    "extends": [
      "./.eslintrc.cjs"
